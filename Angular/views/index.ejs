<!DOCTYPE html>
<html ng-app="TestApp">
  <head>
    <meta charset="utf-8">
    <title>Angular</title>
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.2/angular.js"></script>
  </head>
  <body ng-controller="MainController as vm">
    <h1>Angular</h1>
    <input type="text" ng-model="vm.message"><br>
    {{ vm.message }}
    <br>
    <ul>
      <li ng-repeat="person in vm.people">
        {{ person.name }}
      </li>
    </ul>
    <script type="text/javascript" src="/assets/js/app.js"></script>
  </body>
</html>
